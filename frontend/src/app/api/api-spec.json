{
  "openapi": "3.0.0",
  "info": {
    "title": "Catering App API",
    "description": "Endpoint Format Request Dan Responsenya",
    "version": "1.0.0"
  },
  "servers": [
    {
      "url": "http://localhost:8080",
      "description": "Server lokal"
    }
  ],
  "paths": {
    "/api/caterings": {
      "get": {
        "summary": "Mendapatkan semua catering",
        "responses": {
          "200": {
            "description": "Sukses",
            "content": {
              "application/json": {
                "example": {
                  "status": "success",
                  "data": [
                    {
                      "id": 1,
                      "seller_id": 1
                    }
                  ]
                }
              }
            }
          }
        }
      },
      "post": {
        "summary": "Membuat catering baru",
        "requestBody": {
          "required": true,
          "content": {
            "application/json": {
              "example": {}
            }
          }
        },
        "responses": {
          "201": {
            "description": "Catering berhasil dibuat",
            "content": {
              "application/json": {
                "example": {
                  "status": "success",
                  "data": {
                    "id": 1,
                    "seller_id": 1
                  }
                }
              }
            }
          }
        }
      }
    }
  }
}
